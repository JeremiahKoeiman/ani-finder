{
  "name": "anifinder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:all": "npm run test && npm run e2e",
    "e2e": "npx playwright test",
    "e2e:ui": "npx playwright test --ui",
    "e2e:report": "npx playwright show-report",
    "e2e:allure": "allure serve allure-results",
    "lint": "ng lint",
    "generate": "ng generate",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:serve": "compodoc -s",
    "docs": "compodoc -p tsconfig.doc.json -s",
    "visualize-circular-dependencies": "madge --circular --extensions ts ./src/app",
    "update-npm": "npm-check-updates -i",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:copy": "npx cap copy",
    "cap:update": "npx cap update",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start -- --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeremiahKoeiman/ani-finder.git"
  },
  "keywords": [],
  "author": "Jeremiah Koeiman",
  "bugs": {
    "url": "https://github.com/JeremiahKoeiman/ani-finder/issues"
  },
  "homepage": "https://github.com/JeremiahKoeiman/ani-finder#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@capacitor/android": "7.0.1",
    "@capacitor/app": "7.0.0",
    "@capacitor/core": "7.0.1",
    "@capacitor/haptics": "7.0.0",
    "@capacitor/ios": "7.0.1",
    "@capacitor/keyboard": "7.0.0",
    "@capacitor/status-bar": "7.0.0",
    "@ionic/angular": "^8.0.0",
    "@ngneat/spectator": "^19.4.1",
    "ionicons": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular-eslint/builder": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/schematics": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/language-service": "^19.0.0",
    "@capacitor/cli": "7.0.1",
    "@compodoc/compodoc": "^1.1.26",
    "@ionic/angular-toolkit": "^12.0.0",
    "@playwright/test": "^1.51.0",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
